coinmon| grep -v "Change" | grep -v "Data source" |sed 's/┼/ /g'| sed 's/├/ /g'| sed 's/┤/ /g'| sed 's/┴/ /g'| sed 's/─/ /g'| sed 's/│/ /g'|  sed 's/┘/ /g'|   sed 's/└/ /g'|  sed 's/┌/ /g'|  sed 's/┬/ /g'|  sed 's/┐/ /g'| gsed -r "s/\x1B\[([0-9]{1,3}(;[0-9]{1,2})?)?[mGK]//g" | sed '/^$/d' | sed '/^[[:space:]]*$/d' | sed '/^[[:space:]]*$/d' | gsed -r '/^\s*$/d' | sed '/^[[:space:]]*$/d' | awk '{$1=""}1' | awk '{$1=$1}1' | awk '{$5=""}5' | awk '{$5=$5}5' | awk '{$6=""}6' | awk '{$6=$6}6' | awk '{$4=""}4' | awk '{$4=$4}4' | awk '{$4=""}4' | awk '{$4=$4}4' | sed -e 's/$/     /' | awk '$2 ="$"$2 {print}'| awk '$3 =$3"     " {print}'